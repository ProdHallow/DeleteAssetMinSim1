for _, parent in pairs(parents) do
    if parent then
        for _, child in pairs(parent:GetChildren()) do
            child:Destroy()
        end
    end
end

local part = Instance.new("Part")
part.Size = Vector3.new(4, 1, 4)
part.Position = Vector3.new(41.96063995361328, 10.42, -1239.646484375)
part.Anchored = true
part.Transparency = 1
part.Parent = workspace

local playerGui = game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui
playerGui.StatsFrame.Tokens.Size = UDim2.new(0.5, 0, 0.100000001, 0)
playerGui.StatsFrame.Inventory.Size = UDim2.new(0.5, 0, 0.100000001, 0)
playerGui.StatsFrame.Coins.Size = UDim2.new(0.5, 0, 0.100000001, 0)

local blocksFolder = workspace.Blocks

local function setTransparency()
    for _, block in ipairs(blocksFolder:GetChildren()) do
        local colorPart = block:FindFirstChild("ColorPart")
        if colorPart and colorPart:IsA("BasePart") then
            colorPart.Transparency = 1
        end
    end
end

while true do
    setTransparency()
    wait()
end

do
